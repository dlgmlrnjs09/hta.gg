<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="gg.hta.lol.mapper.SummonerMapper">
	<insert id="addSummoner">
		<selectKey keyProperty="snum" order="BEFORE" resultType="long">
			select snum_seq.nextval from dual
		</selectKey>
		insert into summoner values(#{snum},#{snickname},#{slevel},#{icon})
	</insert>
</mapper>